{
    "name": "whitebit/php-sdk",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "WhiteBIT\\Sdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2",
        "saloonphp/saloon": "^2.0",
        "ext-json": "*",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/collections": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "pestphp/pest": "2.x-dev",
        "mockery/mockery": "2.0.x-dev",
        "laravel/pint": "dev-main",
        "phpstan/phpstan": "1.11.x-dev",
        "rector/rector": "^1.0.4"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WhiteBIT\\Sdk\\WhiteBITServiceProvider"
            ]
        }
    }
}
